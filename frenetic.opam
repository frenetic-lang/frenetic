opam-version: "1.2"
available: [ ocaml-version >= "4.05.0" ]
version: "5.0.2"
maintainer: "Steffen Smolka <smolka@cs.cornell.edu>"
authors: "Arjun Guha <arjun@cs.umass.edu>, Nate Foster <jnfoster@cs.cornell.edu>, Steffen Smolka <smolka@cs.cornell.edu>"
homepage: "http://frenetic-lang.org"
bug-reports: "https://github.com/frenetic-lang/frenetic/issues"
dev-repo: "https://github.com/frenetic-lang/frenetic.git"

build: ["jbuilder" "build" "-p" name "-j" jobs]
build-doc: ["jbuilder" "build" "@doc" "-p" name "-j" jobs]
build-test: ["jbuilder" "runtest" "-p" name "-j" jobs]

depends: [
  "cppo"
  "jbuilder" {>="1.0+beta13"}
  "odoc"
  #########################
  "async"  {>= "v0.10.0" & < "v0.11.0"}
  "async_extended" {>= "v0.10.0" & < "v0.11.0"}
  "base64" {>= "2.0.0"}
  "cohttp"
  "cohttp-async"
  "core"   {>= "v0.10.0" & < "v0.11.0"}
  "cstruct" {>= "1.0.1"}
  "cstruct-async"
  "ipaddr" {>= "2.5.0"}
  "menhir"
  "mparser"
  "ocamlgraph" {>= "1.8.7"}
  "ppx_compare"
  "ppx_core"
  "ppx_cstruct"
  "ppx_deriving" {>= "4.2"}
  "ppx_driver"
  "ppx_enumerate"
  "ppx_fields_conv"
  "ppx_metaquot"
  "ppx_sexp_conv"
  "ppx_tools_versioned"
  "sedlex" {>= "1.99.4"}
  "sexplib"
  "tcpip"
  "yojson" {>= "1.2.0"}
]
